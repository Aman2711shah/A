/**
 * npm i firebase-admin
 * GOOGLE_APPLICATION_CREDENTIALS must point to your service account JSON.
 * node seedServices.js
 */
const fs = require('fs');
const admin = require('firebase-admin');

admin.initializeApp({ /* uses GOOGLE_APPLICATION_CREDENTIALS */ });
const db = admin.firestore();

(async () => {
  const data = JSON.parse(fs.readFileSync('./firestore_service_categories.json', 'utf8'));

  for (const category of data) {
    const catRef = db.collection('service_categories').doc();
    await catRef.set({
      name: category.name,
      subtitle: category.subtitle || '',
      overview: category.overview || '',
      benefits: Array.isArray(category.benefits) ? category.benefits : [],
      requirements: Array.isArray(category.requirements) ? category.requirements : [],
      // tip: add slug if you want stable IDs
    });

    const types = Array.isArray(category.types) ? category.types : [];
    for (const t of types) {
      // use type name as doc id (slug) if you want stable URLs
      const typeRef = catRef.collection('types').doc();
      await typeRef.set({
        name: t.name,
        description: t.description || '',
        subServices: (t.subServices || []).map(ss => ({
          name: ss.name,
          premiumCost: ss.premiumCost ?? null,
          standardCost: ss.standardCost ?? null,
          premiumTimeline: ss.premiumTimeline || '',
          standardTimeline: ss.standardTimeline || '',
          documents: Array.isArray(ss.documents) ? ss.documents : []
        }))
      });
    }
  }

  console.log('âœ… Seed complete');
  process.exit(0);
})();
